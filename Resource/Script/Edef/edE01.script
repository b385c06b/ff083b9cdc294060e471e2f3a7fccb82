# 0x0112

@ 1

EA 2 ENAC_REPOSITION_T
	60

@ @@>%= 240 240 96

SD [i] 5
LOOP [i]
{
	SD [j] 3
	LOOP [j]
	{
		B ENX ENY
			false j * 60 - 60 i *f 0.2 +f 2.0
			6 14 8 0 0xff
	}
}
ENSAIM RAND 0

@ @@>%<% 120 240 80 5

SD [toffset] EN@ % 40
SD [tflag] EN@ % 80 < 40
SD [tangle] SIGN tflag * toffset * 75
SD [tangle] ENAIMX + tangle
BC 12 tangle toffset * -3
	ENX ENY toffset /f 20.0 +f 1.5
	2 tflag + 11 8 0


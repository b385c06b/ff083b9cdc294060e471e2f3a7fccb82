# 0x0108

@ 1

EA 5 ENAC_OVERPLAYER_CXYT
	120 CenterBossX CenterBossY 75

@ @@>% 120 75

BC 16 ENAMAP 0 0
	ENX ENY 2.5
	6 14 8 0

@ @@>%<% 120 270 90 45

SD [i] 4
SD [rangle] RAND
LOOP [i]
{
	GB 0x8108 0xff 0xff
		ENX ENY i * 9000 + rangle 0.6
		0 10 240
	GHSAIM EN@ % 180 0
}


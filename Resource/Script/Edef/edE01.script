# 0x0116

@ 1

EA 5 ENAC_OVERPLAYER_CXYT
	120 CenterBossX CenterBossY 75

@ @@% 180

SD [tangle] ENAMAP -6000

SD [i] 3
IF EN@ - 180 !
{
	SD [i] 1
	SD [tangle] tangle + 6000
}
LOOP [i]
{
	GB 0x8116 0xff 0xff
		ENX ENY i * 6000 + tangle 2.0
		0 10 240
	GHSAIM RAND EN@ % 360 > 0
}
